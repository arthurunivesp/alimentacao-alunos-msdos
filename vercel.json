{
  "version": 2,
  "builds": [
    {
      "src": "__init__.py",
      "use": "@vercel/python",
      "config": { "pythonVersion": "3.12" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "__init__.py"
    }
  ],
  "buildCommand": "bash build.sh",
  "env": {
    "PIP_NO_BINARY": "reportlab"
  }
}




